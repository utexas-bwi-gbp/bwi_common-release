<package>
  <name>utexas_gdc</name>
  <version>0.2.0</version>
  <description>Simulation environment for the Gates Dell Complex at the
    University of Texas At Austin</description>

  <license>BSD</license>

  <url type="repository">https://github.com/utexas-bwi/bwi_common</url>
  <url type="bugtracker">https://github.com/utexas-bwi/bwi_common/issues</url>

  <maintainer email="piyushk@gmail.com">Piyush Khandelwal</maintainer>
  <author email="piyushk@gmail.com">Piyush Khandelwal</author>

  <buildtool_depend>catkin</buildtool_depend>

  <run_depend>gazebo_ros</run_depend>
  <run_depend>map_server</run_depend>
  <run_depend>bwi_gazebo_entities</run_depend>

  <!-- testing dependencies -->
  <build_depend>roslaunch</build_depend>

  <export>
    <gazebo_ros gazebo_media_path="${prefix}"/>
  </export>

</package>
